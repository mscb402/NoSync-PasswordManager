{ 
   "name": "密码管理器", 
   "manifest_version":2,
   "version": "1.0", 
   "description": "密码管理器Chrome扩展", 
   
   "permissions": [
   	  "tabs",
   	  "storage",
   	  "<all_urls>"
   ],
   
   "options_page": "options.html",
   
   "browser_action": { 
      "default_icon": "icon.png", 
      "default_popup": "popup.html" 
   },
   
    "background" : {
      "scripts" : [
      	"popup.js" 
    ]
  } 

}
